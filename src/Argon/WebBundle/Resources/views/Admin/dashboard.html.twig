{% extends "ArgonWebBundle::layout_admin.html.twig" %}
{% set active = "dashboard" %}

{% block title %}{{ parent() }} - {{ "admin.dashboard.title"|trans }}{% endblock %}

{% block top %}
<nav aria-label="{{ "admin.you_are_here"|trans }}" role="navigation">
  <ul class="breadcrumbs">
    <li>{{ "admin.dashboard.title"|trans }}</li>
  </ul>
</nav>
{% endblock %}

{% block content %}
<article>
  <h3>{{ "admin.dashboard.title"|trans }}</h3>
  <dl>
    <dt>{{ "admin.dashboard.version"|trans }}</dt>
    <dd>{{ project.version }}</dd>
  </dl>
</article>
{% endblock %}