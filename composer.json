{
    "name": "abenteuerritter/argonbook",
    "license": "MIT",
    "type": "project",
    "description": "Alternate Reality Games Oriented Network",
    "autoload": {
        "psr-4": {
            "Argon\\": "src/Argon/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^3.0",
        "doctrine/orm": "^2.7",
        "doctrine/doctrine-bundle": "^1.12",
        "symfony/swiftmailer-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "stof/doctrine-extensions-bundle": "^1.2",
        "friendsofsymfony/user-bundle": "^2.0@beta",
        "friendsofsymfony/message": "^1.0@dev",
        "knplabs/knp-paginator-bundle": "^2.5",
        "cebe/markdown": "^1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
