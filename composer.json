{
    "name": "abenteuerritter/argonbook",
    "license": "MIT",
    "type": "project",
    "description": "Alternate Reality Games Oriented Network",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.0.*",
        "paragonie/random_compat": "~1.0|~2.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/assetic-bundle": "^2.7",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "knplabs/knp-paginator-bundle": "^2.5",
        "stof/doctrine-extensions-bundle": "^1.2",
        "necolas/normalize.css": "^4.1",
        "capua/sass-utils": "dev-master",
        "waiting-for-dev/zass": "dev-master"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "necolas/normalize.css",
                "license": "MIT",
                "version": "4.1.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/necolas/normalize.css/archive/4.1.1.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/necolas/normalize.css",
                    "reference": "4.1.1"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "capua/sass-utils",
                "license": "MIT",
                "version": "dev-master",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/capua/sass-utils/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/capua/sass-utils.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "waiting-for-dev/zass",
                "license": "LGPL-3.0",
                "version": "dev-master",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/waiting-for-dev/zass/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/waiting-for-dev/zass.git",
                    "reference": "master"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
